{{> header}}

<div class="container" id="main-content">
    <h1 class="header center">{{title}}</h1>
    <div class="row center">
        <h5 class="header col s12 light">We pick the restaurants so you don't have to. What are you in the mood for?</h5>
    </div>
    <form method="post" action="" id="oderForm">
    <div class="card">
        <div class="card-content">
            <span class="card-title">Enter the cuisine type you would like options for</span>
            <input type="text" >
        </div>
        <div class="card-action">
            <button type="submit" class="btn waves-effect waves-light"><i class="material-icons right">search</i>Search</button>
        </div>
    </div>
    </form>
</div>

{{> footer}}